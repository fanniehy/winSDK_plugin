/* plugin/components/win-fullscreen/win-fullscreen.wxss */
.container {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  z-index: 9999999;
}

.win_fullscreen_video {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.win_fullscreen_video video {
  width: 100%;
  height: 100%;
}
.ad_picture_container {
  width: 100%;
  height: 100%;
}